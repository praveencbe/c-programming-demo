 * Purpose: Array Traversal of 1 D array
 * Language:  C
 * Created:  16-09-2021
 */
  
#include <stdio.h>
#include <stdlib.h>

int main()
{
    int num, *arr, i;
    scanf("%d", &num);
    arr = (int*) malloc(num * sizeof(int));
    for(i = 0; i < num; i++) {
        scanf("%d", arr + i);
    }

    for(int i = 0, j = num-1; i <= j; i++, j --) {
        int t = arr[i];
        arr[i] = arr [j];
        arr[j] = t;
    }

    for(i = 0; i < num; i++)
        printf("%d ", *(arr + i));
    return 0;
}


/* output:
5
1 2 3 4 5
5 4 3 2 1 

...Program finished with exit code 0
Press ENTER to exit console.
*/
